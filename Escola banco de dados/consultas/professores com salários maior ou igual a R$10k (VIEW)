--Nome dos professores com salários maior ou igual a R$10.000,00 e quais turmas eles ensinam

--criei a VIEW com o nome 'prof_salario'
CREATE VIEW prof_salario as 

--selecionei as colunas 'nome', 'salario' e 'num_turma' da tabela "professor'
SELECT nome, salario, num_turma 
from professor

--especifiquei o salario que é maior ou igual a 10 mil
WHERE salario >= 10000
